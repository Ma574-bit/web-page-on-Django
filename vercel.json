{
    "version": 2,
    "builds": [
        {
            "src": "Howlong/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Howlong/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "Howlong.settings"
    }
}